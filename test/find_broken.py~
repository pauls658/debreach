good = "facebook_good"
bad = "facebook_broken"

with open(good, 'rb') as gfd, open(bad, 'rb') as bfd:
    counter = 0
    while 1:
        g = gfd.readline()
        b = bfd.readline()
        if not g or not b:
            break

        if g == b:
            counter += 1
        else:
            break
    print counter
